<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express skills</title>
</head>
<body>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%= skills.id %>"><%= skills.skill %></a>
            - 
          <%= skills.have ? 'have' : 'need' %>
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add a new skill</a>
  </body>
</html>